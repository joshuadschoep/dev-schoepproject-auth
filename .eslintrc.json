{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": ["standard-with-typescript", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": ["build", "node_modules", "jest.config.js", "webpack.config.ts"],
    "rules": {
        "@typescript-eslint/explicit-function-return-type": "off",
        "no-var": "off"
    }
}
